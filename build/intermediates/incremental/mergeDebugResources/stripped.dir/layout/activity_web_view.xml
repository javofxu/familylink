<?xml version="1.0" encoding="utf-8"?>
                                                                   
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"
        android:orientation="vertical" android:tag="layout/activity_web_view_0" xmlns:android="http://schemas.android.com/apk/res/android">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_50"
            android:background="@color/color_white">

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:text="@string/privacy_agree"
                android:textColor="@color/top_bar_title"
                android:textSize="@dimen/title_size" />

            <ImageView
                android:id="@+id/iv_web_view_back"
                android:layout_width="@dimen/back_image_width"
                android:layout_height="@dimen/back_image_height"
                android:padding="@dimen/margin_3"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/back_image_marginLeft"
                android:src="@mipmap/back"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/margin_1"
            android:background="@color/top_bar_line" />
        <WebView
            android:id="@+id/web_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
        </WebView>
    </LinearLayout>
         